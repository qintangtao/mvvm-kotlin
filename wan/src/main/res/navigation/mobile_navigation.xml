<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <tab_fragment
        android:id="@+id/navigation_home"
        android:name="com.qin.wan.ui.main.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <tab_fragment
        android:id="@+id/navigation_system"
        android:name="com.qin.wan.ui.main.system.SystemFragment"
        android:label="@string/system"
        tools:layout="@layout/fragment_system" />

    <tab_fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.qin.wan.ui.main.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <tab_fragment
        android:id="@+id/navigation_notifications"
        android:name="com.qin.wan.ui.main.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <tab_fragment
        android:id="@+id/navigation_mine"
        android:name="com.qin.wan.ui.main.mine.MineFragment"
        android:label="@string/mine"
        tools:layout="@layout/fragment_mine" />

</navigation>